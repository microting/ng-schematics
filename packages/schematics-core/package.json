{
  "name": "@microting/schematics-core",
  "version": "0.5.0",
  "description": "Schematic Utilities",
  "scripts": {
    "build": "rm -rf dist && tsc",
    "test:unit": "ava"
  },
  "keywords": [
    "schematics",
    "schematic utilities"
  ],
  "author": "Kevin Schuchard",
  "license": "MIT",
  "dependencies": {
    "@angular-devkit/core": "13.1.2",
    "@angular-devkit/schematics": "11.2.15",
    "@phenomnomnominal/tsquery": "4.1.1",
    "@schematics/angular": "11.2.15",
    "fuzzy": "0.1.3",
    "inquirer": "8.2.0",
    "inquirer-autocomplete-prompt": "1.4.0",
    "inquirer-fuzzy-path": "2.3.0",
    "lodash": "4.17.21",
    "rxjs": "6.6.7",
    "strip-json-comments": "3.1.1",
    "typescript": "3.9.10"
  },
  "files": [
    "dist"
  ],
  "main": "./dist/index.js",
  "typings": "./dist/index.d.ts",
  "ava": {
    "files": [
      "src/**/*.spec.ts"
    ],
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ]
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@types/inquirer": "8.1.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/microting/ng-schematics/tree/master/packages/@microting/schematics-core",
    "directory": "packages/@microting/schematics-core"
  }
}
