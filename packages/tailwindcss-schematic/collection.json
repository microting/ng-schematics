{
  "$schema": "../../node_modules/@angular-devkit/schematics/collection-schema.json",
  "schematics": {
    "ng-add": {
      "description": "Add Tailwind to an application.",
      "factory": "./dist/index#tailwindSchematic",
      "schema": "./dist/schema.json"
    },
    "demo": {
      "description": "Add Tailwind demo markup.",
      "factory": "./dist/demo",
      "schema": "./dist/schema.json"
    },
    "__test": {
      "description": "Development test for Tailwind Schematic",
      "factory": "./dist/e2e",
      "schema": "./dist/schema.json",
      "private": true
    }
  }
}
